 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 */

// ( A NAND A ) NAND ( B NAND B )

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
	Nand(a=a,b=a,out=outNand1);
	Nand(a=b,b=b,out=outNand2);
	Nand(a=outNand1,b=outNand2,out=out);
}
