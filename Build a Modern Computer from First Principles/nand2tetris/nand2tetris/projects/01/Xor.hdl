/**
 * Exclusive-or gate:
 * out = not (a == b)
 */
 
// (not A and B) or (A and not B)

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
	Not(in=a,out=notA);
	Not(in=b,out=notB);
	And(a=notA,b =b, out = notAandB);
	And(a=a,b =notB, out = notBandA);
	Or(a=notAandB,b=notBandA,out=out);
}